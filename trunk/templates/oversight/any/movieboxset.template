<html>
    <head>
        <title>Oversight ([:HOSTNAME:]) - [:TITLE:] Boxset</title>
		[:INCLUDE_TEMPLATE(styles_and_meta):]
    </head>
[:IF($ovs_display_fanart):]
    <body [onloadset=:START_CELL:] focuscolor=#66CCFF focustext=black class=menu_background background="[:FANART_URL(default.jpg):]" >
    <table class=main width=100%>
    	<tr valign=bottom>
[:ELSE:]
    <body [onloadset=:START_CELL:] focuscolor=#66CCFF focustext=black class=menu_background background="[:BACKGROUND_URL(default.jpg):]">
    <table class="main" width="100%">
    	<tr valign="middle">
[:ENDIF:]
        	<td align="center" border="0" height="[:BODY_HEIGHT:]" >
				[:FORM_START:]
				    <center>
				        <table class="main" width="100%" height="100%">
				            <tr>
				                <td>
						            <table height=50px class="header" background="[:IMAGE_URL(trans50.png):]">
						                <tr>
						                	[:INCLUDE_TEMPLATE(tvboxset_buttons):]
[:IF($@gaya):]	
[:ELSE:]
							                <td align=right>
							                    [:EXTERNAL_URL:]&nbsp;
							                </td>
[:ENDIF:]
						                    <td align=right>
						                        [:INCLUDE_TEMPLATE(box_titles):]
												&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
						                    </td>
						                </tr>
						            </table>
				                </td>
				            </tr>
				            <tr>
				                <td>
				                    <center>
				                        <table >
				                            <tr>
				                                <td >
				                                    <div id=gridWrapper>[:GRID:]</div>
													[:INCLUDE_TEMPLATE(box_footer):]
				                                </td >
				                            </tr>
				                        </table>
				                    </center>
				                </td>
				            </tr>
				        </table>
				    </center>
				[:FORM_END:]
			</td>
		</tr>
	</table>
	[:INCLUDE_TEMPLATE(box_closing_scripts):]
	</body>
</html>
