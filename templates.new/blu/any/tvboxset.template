<html>
    <head>
        <title>Oversight ([:HOSTNAME:]) - [:TITLE:]</title>
		[:INCLUDE_TEMPLATE(styles_and_meta):]
    </head>
    <body [onloadset=:START_CELL:] focuscolor=#66CCFF focustext=black class=menu_background background="[:BACKGROUND_URL(default.jpg):]">
[:FORM_START:]
            <table class=header width=100% background="[:IMAGE_URL(bluemenu2.png):]" >
                <tr>
                    [:INCLUDE_TEMPLATE(tvboxset_buttons):]
                    <td width=80% align=right>
                    	<span class=menubarTitle><b>[:TITLE:]</b>&nbsp;&nbsp;[:PAGE:]/[:PAGE_MAX:]</span>
                    </td>
					[:INCLUDE_TEMPLATE(right_button):]
                </tr>
            </table>
            <center>
            	<br/>
            	<div id=gridWrapper>
[:IF($@sd):]
					[:GRID:]
[:ELSE:]
            		[:GRID(rows=>1,cols=>6,offset=>0):]
[:ENDIF:]
            	</div>
            </center>
            [:INCLUDE_TEMPLATE(box_footer):]
		[:FORM_END:]
[:IF($@sd):]
		<div class=titleLayer>
			[:INCLUDE_TEMPLATE(box_titles):]
		</div>
[:ELSE:]
		<table class=infoTable width=100%>
			<tr>
				<td width=100% colspan=2 height=35px>&nbsp;</td>
			</tr>		
			<tr>
				<td width=50% valign=top>[:PLOT:]</td>
				<td width=50% rowspan=2 align=right><img class=fanartImage src="[:FANART_URL(lightwave.jpg):]"/></td>
			</tr>
			<tr>
				<td width=50% valign=bottom>
					[:INCLUDE_TEMPLATE(box_titles):]
				</td>
			</tr>
		</table>
[:ENDIF:]
		[:INCLUDE_TEMPLATE(box_closing_scripts):]
    </body>
</html>
